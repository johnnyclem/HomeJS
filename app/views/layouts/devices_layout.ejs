<!DOCTYPE html>
<html lang="en" class="dark">
<head>
   <meta name="viewport" content="width=device-width">
   <link href='http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700' rel='stylesheet' type='text/css'>
   <link rel="shortcut icon" href="content/ico/favicon.ico">
   <link rel="apple-touch-icon-precomposed" sizes="144x144" href="content/ico/apple-touch-icon-144-precomposed.png">
   <link rel="apple-touch-icon-precomposed" sizes="114x114" href="content/ico/apple-touch-icon-114-precomposed.png">
   <link rel="apple-touch-icon-precomposed" sizes="72x72" href="content/ico/apple-touch-icon-72-precomposed.png">
   <link rel="apple-touch-icon-precomposed" href="content/ico/apple-touch-icon-57-precomposed.png">

   <title>HomeJS</title>
  <%- stylesheet_link_tag('bootstrap', 'bootstrap-responsive', 'bootmetro', 'bootmetro-tiles', 'bootmetro-charms', 'bootmetro-ui-light', 'icomoon', 'datepicker', 'daterangepicker', 'style', 'prettify', 'application') %>  

  <%- javascript_include_tag('modernizr-2.6.1.min', 'jquery-1.8.3.min', 'prettify', 'jquery.mousewheel', 'jquery.scrollTo', 'bootstrap', 'bootmetro', 'bootmetro-charms', 'demo', 'holder', 'bootstrap-datepicker', 'daterangepicker', 'rails', 'application') %>
  <%- csrf_meta_tag() %>

</head>

  <body>
    <header id="nav-bar" class="container-fluid">
      <div class="row">
        <% var flash = request.flash('info').pop(); if (flash) { %>
        <div class="alert alert-info">
          <h2><%- flash %></h2>
          <a data-dismiss="alert" class="pull-right">
            <span class="win-ui-light win-commandimage win-commandring" id="close_alert">&#xe03a;</span>
          </a>
        </div>
        <% } %>
        <% flash = request.flash('error').pop(); if (flash) { %>
        <div class="alert alert-error">
          <h2><%- flash %></h2>
          <a data-dismiss="alert" class="pull-right">
            <span class="win-ui-light win-commandimage win-commandring" id="close_alert">&#xe03a;</span>
          </a>
        </div>
        <% }; %>
        <a id="backbutton" class="win-backbutton" href="/"></a>
      </div> <!-- Row Fluid -->

    </header>
    <%- body %>
  </body>
</html>
